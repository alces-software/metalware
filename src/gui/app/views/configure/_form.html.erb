
<%= form_tag do %>
  <% @questions.each do |identifier, question| %>
    <% namespaced_identifier = "answers[#{identifier}]" %>
    <fieldset>
      <%= label_tag(namespaced_identifier, question[:question]) %>
      <%= text_field_tag(namespaced_identifier) %>
    </fieldset>
  <% end %>

  <%= submit_tag('Save') %>
<% end %>
